{
  "name": "next-template",
  "version": "0.1.0",
  "description": "FSD構成のテンプレート",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "20.11.0",
    "npm": "yarn を使用してください",
    "pnpm": "yarn を使用してください",
    "yarn": "4.1.1"
  },
  "packageManager": "yarn@4.1.1",
  "sideEffects": false,
  "main": "",
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "check-code": "run-p type-check lint spell-check",
    "dev": "next dev | pino-pretty",
    "fix": "run-p \"fix:*\"",
    "fix-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --fix --max-warnings 0 --cache",
    "fix-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --write --ignore-path .gitignore",
    "fix:config": "prettier \"./*.{js,json}\" \"./linter-settings/**/*.js\" \"./.hygen/**/*.js\" --write --ignore-path .gitignore",
    "fix:css": "stylelint \"./src/**/*.{ts,tsx}\" --fix --max-warnings 0 --cache --cache-strategy content",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-by-prettier fix-by-eslint",
    "gen": "openapi-zod-client \"./api-yaml/bff_api.yaml\" -o \"./src/common/apiClient/libs/bffApiClient/index.ts\" --strict-objects --default-status auto-correct",
    "hygen:entities": "hygen new entities",
    "hygen:features": "hygen new features",
    "hygen:shared": "hygen new shared",
    "hygen:widgets": "hygen new widgets",
    "lint": "run-p \"lint:*\"",
    "lint-config-by-prettier": "prettier \"./*.{js,json}\" \"./linter-settings/**/*.js\" --check --cache --ignore-path .gitignore",
    "lint-editorconfig": "editorconfig-checker",
    "lint-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings 0 --cache",
    "lint-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --check --cache",
    "lint:config": "run-s lint-editorconfig lint-config-by-prettier",
    "lint:css": "stylelint \"./src/**/*.{ts,tsx}\" --max-warnings 0 --cache-strategy content",
    "lint:pack": "fixpack --dryRun",
    "lint:ts": "run-s lint-ts-by-eslint lint-ts-by-prettier",
    "mock": "prism mock -h 0.0.0.0 -p 4010 ./api-yaml/bff_api.yaml",
    "prepare": "husky install",
    "spell-check": "cspell \"./src/**/*.{ts,tsx}\" --gitignore --no-progress",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "jest --coverage",
    "type-check": "tsc"
  },
  "dependencies": {
    "@hookform/resolvers": "3.4.2",
    "@sentry/browser": "5.10.2",
    "@sentry/node": "5.30.0",
    "@zodios/plugins": "10.6.0",
    "axios": "1.6.8",
    "date-fns": "3.6.0",
    "dd-trace": "5.17.0",
    "jotai": "2.7.1",
    "material-icons": "1.13.12",
    "next": "14.1.4",
    "pino": "8.19.0",
    "pino-pretty": "11.0.0",
    "polished": "4.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.51.5",
    "ress": "5.0.2",
    "styled-components": "6.1.8",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@stoplight/prism-cli": "5.7.0",
    "@storybook/addon-actions": "8.0.5",
    "@storybook/addon-essentials": "8.0.5",
    "@storybook/addon-interactions": "8.0.5",
    "@storybook/addon-links": "8.0.5",
    "@storybook/addon-onboarding": "8.0.5",
    "@storybook/addon-viewport": "8.0.5",
    "@storybook/blocks": "8.0.5",
    "@storybook/nextjs": "8.0.5",
    "@storybook/react": "8.0.5",
    "@storybook/test": "8.0.5",
    "@storybook/testing-library": "0.2.2",
    "@stylistic/eslint-plugin": "1.6.3",
    "@swc/core": "1.4.8",
    "@swc/jest": "0.2.36",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.2",
    "@testing-library/react-hooks": "8.0.1",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.27",
    "@types/react": "18.2.65",
    "@types/react-dom": "18.2.22",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "cspell": "8.6.1",
    "dotenv": "16.4.5",
    "editorconfig-checker": "5.0.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-strict-check": "0.0.24",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-storybook": "0.8.0",
    "eslint-plugin-strict-check": "0.0.20",
    "eslint-plugin-strict-dependencies": "1.3.3",
    "eslint-plugin-unicorn": "51.0.1",
    "fixpack": "4.0.0",
    "html-loader": "5.1.0",
    "husky": "9.0.1",
    "hygen": "6.2.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jsdom-testing-mocks": "1.13.0",
    "msw": "2.2.13",
    "npm-run-all2": "6.1.2",
    "openapi-zod-client": "1.16.4",
    "postcss": "8.4.35",
    "postcss-styled-syntax": "0.6.4",
    "prettier": "3.2.5",
    "storybook": "8.0.5",
    "storybook-addon-pseudo-states": "3.0.0",
    "storybook-addon-swc": "1.2.0",
    "string-replace-loader": "3.1.0",
    "stylelint": "16.2.1",
    "stylelint-config-standard": "36.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.8.0",
    "stylelint-no-unsupported-browser-features": "8.0.1",
    "stylelint-order": "6.0.4",
    "typescript": "5.3.3",
    "undici": "5.26.2"
  }
}
